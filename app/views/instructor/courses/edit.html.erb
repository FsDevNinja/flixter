<br />
<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Edit your course</h1>
  <%= simple_form_for @course, url: instructor_courses_path do |f| %>
  <%= f.input :title %>
  <%= f.input :image %>
  <%= f.input :description %>
  <%= f.input :cost %>
  <br />
  <%= f.submit "Update", class: 'btn btn-primary' %>
  <br />
  <% end %>
  <br />
</div>
